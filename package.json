{
    "name": "mydao",
    "version": "1.0.0",
    "description": "Basic voting DAO smart contract for research purposes",
    "main": "truffle-config.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "test": "truffle test --reporter eth-gas-reporter",
        "coverage": "truffle run coverage && cat coverage/lcov.info | coveralls",
        "lint": "solhint --max-warnings 0 \"contracts/**/*.sol\" -f stylish"
    },
    "keywords": [
        "ERC-20",
        "DAO"
    ],
    "author": "Esteban Hugo Somma",
    "license": "MIT",
	"repository": "github:ehsomma/mydao",
    "bugs": {
        "url": "https://github.com/ehsomma/mydao/issues"
    },
    "homepage": "https://github.com/ehsomma/mydao",
    "devDependencies": {
        "@openzeppelin/contracts": "^4.5.0",
        "@openzeppelin/test-helpers": "^0.5.15",
        "chai": "^4.3.6",
        "chai-as-promised": "^7.1.1",
        "chai-bn": "^0.3.1",
        "coveralls": "^3.1.1",
        "eth-gas-reporter": "^0.2.24",
        "solidity-coverage": "^0.7.17",
        "truffle-assertions": "^0.9.2"
    }
}
